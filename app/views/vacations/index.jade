extends ../shared/template
block content
  .row
    .col-xs-12
      #map

  .row.backgroundlow
    .col-xs-12
      table.table.table-hover
        thead
          th Name
          th Start
          th End
          th Latitude
          th Longitude
        tbodyr
          each vacation in vacations
            tr
              td= vacation.name
              td= moment(vacation.start).format('dddd, MMM Do YY')
              td= vacation.end
              td= vacation.lat
              td= vacation.lng

block scripts
  script(src='/js/user/home.js')

